<?xml version="1.0"?>
<layout>
	<page name="Main" id="0" background="images/occ_dark_green_nb.png" font="" fg_colour="FFFFFF">
		<field name="top">
			<function>speed</function>
			<button x0="0" y0="0" w="240" h="80"></button>
			<text_center x="95" y="75">0</text_center>
			<font_size>19</font_size>
		</field>
		<field name="top_auxiliary">
			<function>speed_tenths</function>
			<text_center x="210" y="115">0</text_center>
			<font_size>7</font_size>
		</field>
		<field name="top_units">
			<function>speed_units</function>
			<text_center x="205" y="75">km/h</text_center>
			<font_size>2</font_size>
		</field>
		<field name="middle">
			<function>altitude_gps</function>
			<button x0="0" y0="80" w="240" h="140"></button>
			<text_center x="100" y="195">0</text_center>
			<font_size>6</font_size>
		</field>
		<field name="middle_auxiliary">
			<function>altitude_units</function>
			<text_center x="210" y="195">m</text_center>
			<font_size>2</font_size>
		</field>
		<field name="bottom">
			<function>rider_weight</function>
			<text_center x="180" y="280">m</text_center>
			<font_size>4</font_size>
		</field>
	</page>
	<page name="Settings" id="1" background="images/occ_dark_green_settings.png" font="" fg_colour="FFFFFF" >
		<field name="first">
			<function>load_default_layout</function>
			<button x0="0" y0="0" w="240" h="80"></button>
			<text_center x="120" y="40">Default</text_center>
			<font_size>2</font_size>
		</field>
		<field name="second">
			<function>load_lcd_layout</function>
			<button x0="0" y0="79" w="240" h="80"></button>
			<text_center x="120" y="120">LCD</text_center>
			<font_size>2</font_size>
		</field>
		<field name="third">
			<function>load_white_lcd_layout</function>
			<button x0="0" y0="159" w="240" h="80"></button>
			<text_center x="120" y="200">White LCD</text_center>
			<font_size>2</font_size>
		</field>
		<field name="fourth">
			<function>quit</function>
			<button x0="0" y0="239" w="240" h="80"></button>
			<text_center x="120" y="280">Quit</text_center>
			<font_size>2</font_size>
		</field>
	</page>
	<page name="Weather" id="2" background="images/occ_dark_green_frame.png" font="" fg_colour="FFFFFF">
		<field name="latitude">
			<function>latitude</function>
			<text_center x="120" y="30"></text_center>
			<font_size>4</font_size>
		</field>
		<field name="longitude">
			<function>longitude</function>
			<text_center x="120" y="60"></text_center>
			<font_size>4</font_size>
		</field>
		<field name="altitude">
			<function>altitude_gps</function>
			<text_center x="120" y="90"></text_center>
			<font_size>4</font_size>
		</field>
		<field name="altitude">
			<function>altitude_units</function>
			<text_center x="200" y="100"></text_center>
			<font_size>2</font_size>
		</field>
		<field name="bottom_left">
			<function>pressure</function>
			<text_center x="50" y="280">0</text_center>
			<font_size>5</font_size>
		</field>
		<field name="bottom_left_auxiliary">
			<function>pressure_units</function>
			<text_center x="100" y="310"></text_center>
			<font_size>2</font_size>
		</field>
		<field name="bottom_right">
			<function>temperature</function>
			<text_center x="180" y="280"></text_center>
			<font_size>5</font_size>
		</field>
		<field name="bottom_right_auxiliary">
			<function>temperature_units</function>
			<text_center x="220" y="310"></text_center>
			<font_size>2</font_size>
		</field>
	</page>
	<page name="Editor" id="3" background="images/occ_dark_green_nb_editor.png" font="" fg_colour="FFFFFF">
		<field name="top_left">
			<function>ed_cancel</function>
			<button x0="0" y0="0" w="120" h="60"></button>
			<text_center x="60" y="30">Cancel</text_center>
			<font_size>4</font_size>
		</field>
		<field name="top_right">
			<function>ed_accept</function>
			<button x0="121" y0="0" w="120" h="60"></button>
			<text_center x="180" y="30">Accept</text_center>
			<font_size>4</font_size>
		</field>
		<field name="center_header">
			<function>ed_value_description</function>
			<text_center x="120" y="70"></text_center>
			<font_size>2</font_size>
		</field>
		<field name="center">
			<function>ed_value</function>
			<text_center x="120" y="130"></text_center>
			<font_size>6</font_size>
		</field>
		<field name="bottom_upper_left">
			<function>ed_decrease</function>
			<button x0="0" y0="200" w="120" h="60"></button>
			<text_center x="60" y="230">-</text_center>
			<font_size>5</font_size>
		</field>
		<field name="bottom_upper_right">
			<function>ed_increase</function>
			<button x0="120" y0="200" w="120" h="60"></button>
			<text_center x="180" y="230">+</text_center>
			<font_size>5</font_size>
		</field>
		<field name="bottom_lower_left">
			<function>ed_prev</function>
			<button x0="0" y0="260" w="120" h="60"></button>
			<text_center x="60" y="290">&lt;</text_center>
			<font_size>5</font_size>
		</field>
		<field name="bottom_lower_right">
			<function>ed_next</function>
			<button x0="120" y0="260" w="120" h="60"></button>
			<text_center x="180" y="290">&gt;</text_center>
			<font_size>5</font_size>
		</field>
	</page>
</layout>
