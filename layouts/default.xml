<layout>
	<page name="Main" id="page_0" background="images/occ_page_0_background.png" buttons="images/occ_page_0_buttons.png" font="" fg_colour="FFFFFF">
		<field name="top">
			<function>speed</function>
			<button x0="0" y0="0" w="240" h="150"/>
			<text_center x="95" y="75">0</text_center>
			<font_size>19</font_size>
		</field>
		<field name="top_auxiliary">
			<function>speed_tenths</function>
			<text_center x="210" y="115">0</text_center>
			<font_size>7</font_size>
		</field>
		<field name="top_units">
			<function>speed_units</function>
			<text_center x="205" y="75">km/h</text_center>
			<font_size>2</font_size>
		</field>
		<field name="top_right_1">
			<function>satellites_used</function>
			<text_center x="175" y="15"></text_center>
			<font_size>2</font_size>
		</field>
		<field name="top_right_2">
			<function>satellites_visible</function>
			<text_center x="200" y="15"></text_center>
			<font_size>2</font_size>
		</field>
		<field name="top_right_3">
			<function>satellites</function>
			<text_center x="225" y="15"></text_center>
			<font_size>2</font_size>
		</field>
		<field name="middle">
			<function>distance</function>
			<button x0="0" y0="150" w="239" h="89"/>
			<text_center x="100" y="195">0</text_center>
			<font_size>6</font_size>
		</field>
		<field name="middle_auxiliary">
			<function>distance_units</function>
			<text_center x="210" y="195">m</text_center>
			<font_size>2</font_size>
		</field>
		<field name="bottom_right">
			<function>altitude</function>
			<button x0="120" y0="240" w="119" h="80"/>
			<text_center x="180" y="280">m</text_center>
			<font_size>4</font_size>
		</field>
		<field name="bottom_right_auxiliary">
			<function>altitude_units</function>
			<text_center x="220" y="310"/>
			<font_size>2</font_size>
		</field>
		<field name="bottom_left">
			<function>temperature</function>
			<button x0="0" y0="240" w="119" h="80"/>
			<text_center x="60" y="280"/>
			<font_size>4</font_size>
		</field>
		<field name="bottom_left_auxiliary">
			<function>temperature_units</function>
			<text_center x="100" y="310"/>
			<font_size>2</font_size>
		</field>
	</page>
	<page name="Settings 0" id="settings_0" background="images/occ_settings_0_background.png" buttons="images/occ_settings_0_buttons.png" font="" fg_colour="FFFFFF">
		<field name="first">
			<function>load_current_layout</function>
			<button x0="0" y0="0" w="240" h="80"/>
			<text_center x="120" y="40">Load current layout</text_center>
			<font_size>2</font_size>
		</field>
		<field name="second">
			<function>load_default_layout</function>
			<button x0="0" y0="79" w="240" h="80"/>
			<text_center x="120" y="120">Load default layout</text_center>
			<font_size>2</font_size>
		</field>
		<field name="third">
			<function>load_white_lcd_layout</function>
			<button x0="0" y0="159" w="240" h="80"/>
			<text_center x="120" y="200">White LCD</text_center>
			<font_size>2</font_size>
		</field>
	</page>
	<page name="Settings 1" id="settings_1" background="images/occ_settings_1_background.png" buttons="images/occ_settings_1_buttons.png" font="" fg_colour="FFFFFF">
		<field name="first">
			<function>halt</function>
			<button x0="0" y0="0" w="240" h="80"/>
			<text_center x="120" y="40">Halt</text_center>
			<font_size>2</font_size>
		</field>
		<field name="second">
			<function>reboot</function>
			<button x0="0" y0="79" w="240" h="80"/>
			<text_center x="120" y="120">Reboot</text_center>
			<font_size>2</font_size>
		</field>
	</page>
	<page name="Weather" id="page_1" background="images/occ_page_1_background.png" buttons="images/occ_page_1_buttons.png" font="" fg_colour="FFFFFF">
		<field name="top">
			<function>speed_max</function>
			<button x0="0" y0="0" w="240" h="150"/>
			<text_center x="95" y="75"></text_center>
			<font_size>19</font_size>
		</field>
		<field name="top_auxiliary">
			<function>speed_max_tenths</function>
			<text_center x="210" y="115"></text_center>
			<font_size>7</font_size>
		</field>
		<field name="top_units">
			<function>speed_units</function>
			<text_center x="205" y="75">km/h</text_center>
			<font_size>2</font_size>
		</field>
		<field name="bottom_left">
			<function>pressure</function>
			<text_center x="50" y="280">0</text_center>
			<font_size>5</font_size>
		</field>
		<field name="bottom_left_auxiliary">
			<function>pressure_units</function>
			<text_center x="100" y="310"/>
			<font_size>2</font_size>
		</field>
		<field name="bottom_right">
			<function>temperature</function>
			<text_center x="180" y="280"/>
			<font_size>5</font_size>
		</field>
		<field name="bottom_right_auxiliary">
			<function>temperature_units</function>
			<text_center x="220" y="310"/>
			<font_size>2</font_size>
		</field>
	</page>
	<page name="GPS" id="page_2" background="images/occ_page_2_background.png" buttons="images/occ_page_2_buttons.png" font="" fg_colour="FFFFFF">
		<field name="latitude">
			<function>latitude</function>
			<text_center x="120" y="30"/>
			<font_size>4</font_size>
		</field>
		<field name="longitude">
			<function>longitude</function>
			<text_center x="120" y="60"/>
			<font_size>4</font_size>
		</field>
		<field name="altitude">
			<function>altitude_gps</function>
			<text_center x="120" y="90"/>
			<font_size>4</font_size>
		</field>
		<field name="altitude">
			<function>altitude_units</function>
			<text_center x="200" y="100"/>
			<font_size>2</font_size>
		</field>
		<field name="middle_upper_left">
			<function>satellites_used</function>
			<text_center x="50" y="165">0</text_center>
			<font_size>2</font_size>
		</field>
		<field name="middle_upper_center">
			<function>satellites_visible</function>
			<text_center x="120" y="165">0</text_center>
			<font_size>2</font_size>
		</field>
		<field name="middle_upper_right">
			<function>satellites</function>
			<text_center x="190" y="165">0</text_center>
			<font_size>2</font_size>
		</field>
	</page>
	<page name="Editor" id="editor" background="images/occ_editor_background.png" buttons="images/occ_editor_buttons.png" font="" fg_colour="FFFFFF">
		<field name="top_left">
			<function>ed_cancel</function>
			<button x0="0" y0="0" w="119" h="60"/>
			<text_center x="60" y="30">Cancel</text_center>
			<font_size>3</font_size>
		</field>
		<field name="top_right">
			<function>ed_accept</function>
			<button x0="120" y0="0" w="119" h="60"/>
			<text_center x="180" y="30">Accept</text_center>
			<font_size>3</font_size>
		</field>
		<field name="center_header">
			<function>variable_description</function>
			<text_center x="120" y="70"/>
			<font_size>2</font_size>
		</field>
		<field name="center">
			<function>variable_value</function>
			<text_center x="120" y="110"/>
			<font_size>6</font_size>
		</field>
		<field name="center_footer">
			<function>variable_unit</function>
			<text_center x="120" y="180"/>
			<font_size>3</font_size>
		</field>
		<field name="center_left">
			<function>ed_prev_unit</function>
			<button x0="0" y0="140" w="59" h="60"/>
			<text_center x="40" y="130"> </text_center>
			<font_size>3</font_size>
		</field>
		<field name="center_right">
			<function>ed_next_unit</function>
			<button x0="180" y0="140" w="59" h="60"/>
			<text_center x="200" y="130"> </text_center>
			<font_size>3</font_size>
		</field>
		<field name="bottom_upper_left">
			<function>ed_decrease</function>
			<button x0="0" y0="200" w="119" h="60"/>
			<text_center x="60" y="230">-</text_center>
			<font_size>5</font_size>
		</field>
		<field name="bottom_upper_right">
			<function>ed_increase</function>
			<button x0="120" y0="200" w="119" h="60"/>
			<text_center x="180" y="230">+</text_center>
			<font_size>5</font_size>
		</field>
		<field name="bottom_lower_left">
			<function>ed_prev</function>
			<button x0="0" y0="260" w="119" h="60"/>
			<text_center x="60" y="290">Prev</text_center>
			<font_size>3</font_size>
		</field>
		<field name="bottom_lower_right">
			<function>ed_next</function>
			<button x0="120" y0="260" w="119" h="60"/>
			<text_center x="180" y="290">Next</text_center>
			<font_size>3</font_size>
		</field>
	</page>
</layout>
